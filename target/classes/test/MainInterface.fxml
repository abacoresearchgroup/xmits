<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.util.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>


<FlowPane fx:id="fpWindow" alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity" 
          minHeight="-Infinity" minWidth="-Infinity" prefHeight="540.0" prefWidth="500.0" 
          xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" 
          fx:controller="test.Controller">
    <children>
        <Button fx:id="btAddDiagram" alignment="TOP_CENTER" mnemonicParsing="false" 
                prefHeight="0.0" prefWidth="450.0" text="Adicionar Driagramas" 
                onAction="#addAction">
            <FlowPane.margin>
                <Insets />
            </FlowPane.margin>
        </Button>
        <ListView fx:id="lvFilePaths" prefHeight="300.0" prefWidth="450.0" 
                  onKeyPressed="#delPress">
            <FlowPane.margin>
                <Insets top="10.0" />
            </FlowPane.margin>
        </ListView>
        <TextField id="error"  fx:id="error"  prefWidth="450.0">
             <FlowPane.margin>
                <Insets top="10.0" />
            </FlowPane.margin>
        </TextField>
        <Button fx:id="btDel" mnemonicParsing="false" prefWidth="450.0" text="Deletar Diagrama" 
                onAction="#deleteAction">
            <FlowPane.margin>
                <Insets top="15.0" />
            </FlowPane.margin>
        </Button>
        <CheckBox fx:id="cbHasSeqDiagram" mnemonicParsing="false" text="Diagrama SequÃªncia" 
                  onAction="#sequenceDiagram" >
            <FlowPane.margin>
                <Insets right="20.0" top="15.0" />
            </FlowPane.margin>
        </CheckBox>
        <CheckBox fx:id="cbMakeFiles" mnemonicParsing="false" text="Gerar Arquivos" 
                  onAction="#geraArquivo" >
            <FlowPane.margin>
                <Insets top="15.0" />
            </FlowPane.margin>
        </CheckBox>
        <Button fx:id="btRun" mnemonicParsing="false" prefWidth="450.0" text="Executar" 
                onAction="#runAction">
            <FlowPane.margin>
                <Insets top="15.0" />
            </FlowPane.margin>
        </Button>
    </children>
    <padding>
        <Insets top="15.0" />
    </padding>
</FlowPane>
